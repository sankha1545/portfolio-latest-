const animateCarouselImages=()=>{document.querySelectorAll(".image-scroll").forEach(e=>{const t=e.querySelector(".project-images"),o=t.offsetWidth,n=t.querySelectorAll(".h-item-scroll");let a=0;n.forEach(e=>a+=e.offsetWidth+30),n.length<=2||new Animate({scrub:!0,start:"top 30%",end:"bottom center 10%"},{x:0,duration:5,ease:"none"},{x:`-${a-o-30}`,duration:5,ease:"none"},t,t,!1,!1)})};document.addEventListener("DOMContentLoaded",()=>setTimeout(()=>animateCarouselImages(),200));