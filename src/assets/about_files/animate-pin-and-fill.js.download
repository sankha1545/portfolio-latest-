const animatePinAndFill=()=>{   if (window.innerWidth < 768) return; document.body.querySelectorAll(".fill-text").forEach(e=>{e.classList.add("animate-fill-text"),e.querySelectorAll("div").forEach((e,t)=>{new Animate({scrub:1,start:"top top",end:"bottom top"},{},{backgroundPositionX:0,stagger:.05,duration:20,delay:.2*t},e,e,!0,!0)})}),document.body.querySelectorAll(".animate-fill").forEach(e=>{const t=e.querySelector(".creative-studio-header"),n=120;new Animate({start:()=>`top ${n}`,end:"bottom 0",pin:!0,scrub:1,extendTimeline:!0},{},{},e,t,!0,!0)})};document.addEventListener("DOMContentLoaded",()=>setTimeout(()=>animatePinAndFill(),250));