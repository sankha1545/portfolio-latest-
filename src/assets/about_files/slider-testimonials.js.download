const slider_Testimonials=()=>{if(document.body.querySelector(".testimonials .swiper")){const e=new Swiper(".testimonials .swiper",{grabCursor:!0,spaceBetween:30,speed:1600,centeredSlides:!0,initialSlide:0,slideToClickedSlide:!0,pagination:{el:".testimonials-pagination",enabled:!0,clickable:!0},slidesPerView:1.1,breakpoints:{1729:{slidesPerView:2.82},1280:{slidesPerView:2.4},1080:{slidesPerView:2.7},820:{slidesPerView:1.8},768:{slidesPerView:2.5},480:{slidesPerView:1.3}}}),i=document.querySelector(".testimonials-pagination");setTimeout(()=>i.style="display:flex",300),e&&e.slideNext()}};document.addEventListener("DOMContentLoaded",()=>{setTimeout(()=>slider_Testimonials(),1)});